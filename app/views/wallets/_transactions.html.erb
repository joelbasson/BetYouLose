<div id="previews"> 
	
	<% if @transactions.size == 0 %>
		&nbsp
	<% end %>
	
<% for transaction in @transactions do %>
	<%= transaction.description %> on <%= transaction.created_at.strftime("%a %d %b %Y %I:%M %p") %><br />
<% end %>	

<%= will_paginate @transactions, :params => {:controller => "wallets", :action => "show"} %>
</div>